{
 
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "app": {
    
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "piexl-editor",
        "width": 800
      }
    ]
  },
 
  "build": {
    
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:7767",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "identifier": "com.tauri.dev",
  "productName": "piexl-editor",
  "version": "0.1.0"
}
